<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="HappyToolbarView">
        <attr name="title" format="string" />
        <attr name="actionSrc" format="reference" />
    </declare-styleable>

    <declare-styleable name="PinProgressView">
        <attr name="steps" format="integer" />
        <attr name="defaultCircleColor" format="color" />
        <attr name="progressCircleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PinKeyboardButtonView">
        <attr name="btnBackgroundColor" format="color" />
        <attr name="android:text" format="string" />
        <attr name="android:textColor" format="color" />
        <attr name="android:textSize" format="dimension" />
        <attr name="android:src" format="reference" />
    </declare-styleable>

</resources>
